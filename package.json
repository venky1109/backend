{
  "name": "manakirana",
  "version": "1.0.0",
  "type": "module",
  "description": "Grocries eCommerce application",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "data:import": "node backend/seeder",
    "data:merge": "node backend/mergeProducts",
    "data:destroy": "node backend/seeder -d",
    "client": "set REACT_APP_API_KEY=$API_KEY && npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "set REACT_APP_API_KEY=$API_KEY npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "generate-toc": "markdown-toc -i readme.md"
  },
  "author": "Venkatesh Guttula",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "react-phone-number-input": "^3.3.9",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "concurrently": "^7.6.0",
    "markdown-toc": "^1.2.0",
    "nodemon": "^2.0.21"
  }
}
